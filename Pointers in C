#include <stdio.h>

void update(int *a,int *b) {
    int sum=(*a+*b);
    int diff=(*a-*b);
    if (diff<0) {
    diff=-diff;
    }
    printf("%d\n",sum);
    printf("%d\n",diff);
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    

    return 0;
}
